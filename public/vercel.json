{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/несуществующая-страница-12345",
      "status": 404,
      "headers": {
        "Location": "/404"
      }
    },
    {
      "src": "/404",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}