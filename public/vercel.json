{
  "routes": [
    {
      "src": "/несуществующая-страница-12345",
      "status": 404,
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      },
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}